/dts-v1/;

#include "mt7620a.dtsi"

#include <dt-bindings/input/input.h>

/ {
	compatible = "DAP-1520", "ralink,mt7620a-soc";
	model = "D-Link DAP-1520";

	gpio-keys-polled {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 12 1>;
			linux,code = <KEY_WPS_BUTTON>;
		};
		reset {
			label = "reset";
			gpios = <&gpio0 13 1>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		red {
			label = "dap-1520:red:power";
			gpios = <&gpio1 4 1>;
		};
		green {
			label = "dap-1520:green:wlan";
			gpios = <&gpio1 5 1>;
		};
	};
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x30000>;
			read-only;
		};

		partition@30000 {
			label = "u-boot-env";
			reg = <0x30000 0x10000>;
			read-only;
		};

		factory: partition@40000 {
			label = "factory";
			reg = <0x40000 0x10000>;
			read-only;
		};

		partition@50000 {
			label = "firmware";
			reg = <0x50000 0x7b0000>;
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "uartf", "rgmii1";
			ralink,function = "gpio";
		};
	};
};

&pcie {
	status = "okay";
};

&usbphy {
	status = "disabled";
};

&wmac {
	ralink,mtd-eeprom = <&factory 0>;
};
